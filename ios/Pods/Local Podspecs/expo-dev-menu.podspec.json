        "React-CoreModules": [

        ],
        "React": [

        ]
      },
      "platforms": {
        "ios": "15.1"
      }
    }
  ],
  "subspecs": [
    {
      "name": "SafeAreaView",
      "dependencies": {
        "ExpoModulesCore": [

        ]
      },
      "pod_target_xcconfig": {
